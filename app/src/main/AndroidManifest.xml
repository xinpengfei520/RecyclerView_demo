<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xpf.recyclerview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.WelcomeActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.StaggeredGridLayoutActivity" />
        <activity android:name=".activity.AddHeadRecyclerViewActivity" />
        <activity android:name=".activity.LinearLayoutActivity" />
        <activity android:name=".activity.onViewAttachedToWindow" />
        <activity android:name=".activity.MultiItemActivity" />
        <activity android:name=".activity.MultipleItemQuickActivity" />
        <activity android:name=".activity.SectionActivity" />
        <activity android:name=".activity.DragAndSwipeActivity" />
        <activity android:name=".activity.ItemClickEventActivity" />
        <activity android:name=".activity.ExpandableActivity" />
        <activity android:name=".activity.DataBindingActivity" />
        <activity android:name=".activity.UpFetchActivity" />
        <activity android:name=".activity.SectionMultiItemActivity"></activity>
    </application>

</manifest>